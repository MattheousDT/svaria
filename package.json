		"clsx": "^1.1.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"mdsvex": "^0.9.8",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"sass": "^1.49.0",
